        "Taking care of missing data"
      ],
      "metadata": {
        "id": "iWe-84sET2L5"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.impute import SimpleImputer\n",
        "imputer = SimpleImputer(missing_values = np.nan, strategy = 'mean')\n",
        "imputer.fit(x[:, 2:5])\n",
        "x[:, 2:5] = imputer.transform(x[:, 2:5])\n",
        "print(x)\n",
        "print(y)"
      ],
      "metadata": {
        "id": "Ujie--JoT1aI"
